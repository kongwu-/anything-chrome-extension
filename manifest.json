{
  "manifest_version": 2,

  "name": "anything-chrome-extension",
  "description": "for leevi",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*","https://*/*"
  ],
  "web_accessible_resources":[
    "/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}